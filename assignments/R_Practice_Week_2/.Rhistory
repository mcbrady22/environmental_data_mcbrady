line_point_slope = function(x, x1, y1, slope)
{
get_y_intercept =
function(x1, y1, slope)
return(-(x1 * slope) + y1)
linear =
function(x, yint, slope)
return(yint + x * slope)
return(linear(x, get_y_intercept(x1, y1, slope), slope))
}
View(line_point_slope)
line_point_slope = function(2, 4, 4, -2)
{
get_y_intercept =
function(2, 4, -2)
return(-2 * -2) + 4)
linear =
function(2, 4, -2)
return(4 + 2 * -2)
return(linear(2, get_y_intercept(2, 4, -2), -2))
}
