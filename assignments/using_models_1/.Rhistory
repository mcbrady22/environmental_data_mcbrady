library(readr)
penguins <- read_csv("penguins.csv")
View(penguins)
dat_adelie = subset(penguins, species == "Adelie")
dat_chinstrap = subset(penguins, species == "Chinstrap")
adelie_flip = dat_adelie$flipper_length_mm
chinstrap_flip = dat_chinstrap$flipper_length_mm
shapiro.test(adelie_flip)
shapiro.test(chinstrap_flip)
hist(adelie_flip)
par(mfrow = c(1,2))
hist(adelie_flip, main = "Adelie Penguin Flipper Lengths", xlab = "length (mm)", col = "blue")
hist(chinstrap_flip, main = "Chinstrap Penguin Flipper Lengths", xlab = "length (mm)", col = "red")
hist(adelie_flip, main = "Adelie Penguin \nFlipper Lengths", xlab = "length (mm)", col = "blue")
hist(chinstrap_flip, main = "Chinstrap Penguin \nFlipper Lengths", xlab = "length (mm)", col = "red")
t.test(adelie_flip, chinstrap_flip, alternative = "two.sided")
