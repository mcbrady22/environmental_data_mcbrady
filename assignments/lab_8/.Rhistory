dat_1 =
subset(
dat_all,
select = c(b.sidi, s.sidi))
s_sidi_mean = mean(dat_all$s.sidi, na.rm = TRUE)
s_sidi_sd   = sd(dat_all$s.sidi, na.rm = TRUE)
dat_all$s.sidi.standardized = (dat_all$s.sidi - s_sidi_mean)/s_sidi_sd
n = 10000
result = numeric(n)
for(i in 1:n)
{
index_1 = sample(nrow(dat_1), replace = TRUE)
index_2 = sample(nrow(dat_1), replace = TRUE)
dat_resampled_i =
data.frame(
b.sidi = dat_1$b.sidi[index_1],
s.sidi = dat_1$s.sidi[index_2]
)
fit_resampled_i = lm(b.sidi ~ s.sidi, data = dat_resampled_i)
result[i] = coef(fit_resampled_i)[2]
}
hist(result, main = "MC Simulated Slope Parameters", xlab = "slope parameters", col = "lightcyan1")
abline(v = slope_observed, lty = 1, col = "lightblue2", lwd = 2)
abline(v = quantile(result, c(.05)), lty = 2, col = "cyan2", lwd = 2)
